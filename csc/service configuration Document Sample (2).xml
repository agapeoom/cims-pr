<?xml version="1.0" encoding="UTF-8"?> 
<service-configuration xmlns="urn:3gpp:ns:mcpttServiceConfig:1.0"
		         domain="http://csc.mcptt.com/org.3gpp.mcptt.service-config/users/tel:+82510200011" >
 <common>
  <alias>
    <id>tel:+82510200011</id>
    <minLength>5</minLength> 
  </alias>
  <broadcast-group>
    <num-levels-group-hierarchy >3</num-levels-group-hierarchy >
    <num-levels-user-hierarchy >2</num-levels-user-hierarchy >
  </broadcast-group>
 </common>

 <on-network>
   <emergency-call>
     <private-cancel-timeout>2</private-cancel-timeout>
     <group-time-limit>3</group-time-limit>
   </emergency-call>
   <private-call>
     <hang-time>3</hang-time>
     <max-duration-with-floor-control>3</max-duration-with-floor-control>
     <max-duration-without-floor-control>3</max-duration-without-floor-control>
   </private-call>
   <num-levels-priority-hierarchy >3</num-levels-priority-hierarchy >
   <transmit-time>
     <time-limit>3</time-limit >
     <time-warning>3</time-warning >
   </transmit-time>
   <hang-time-warning>3</hang-time-warning >
   <floor-control-queue>
     <depth>3</depth>
     <max-user-request-time>3</max-user-request-time>
   </floor-control-queue>
   <fc-timers-counters>
     <T1-end-of-rtp-media >3</T1-end-of-rtp-media>
     <T3-stop-talking-grace >3</T3-stop-talking-grace>
     <T7-floor-idle >3</T7-floor-idle>
     <T8-floor-revoke >3</T8-floor-revoke>
     <T11-end-of-RTP-dual >3</T11-end-of-RTP-dual>
     <T12-stop-talking-dual >3</T12-stop-talking-dual>
     <T15-conversation >3</T15-conversation>
     <T16-map-group-to-bearer >3</T16-map-group-to-bearer>
     <T17-unmap-group-to-bearer >3</T17-unmap-group-to-bearer>
     <T20-floor-granted >3</T20-floor-granted>
     <T55-connect >3</T55-connect>
     <T56-disconnect >3</T56-disconnect>
     <C7-floor-idle >3</C7-floor-idle>
     <C17-unmap-group-to-bearer >3</C17-unmap-group-to-bearer>
     <C20-floor-granted >3</C20-floor-granted>
     <C55-connect >3</C55-connect>
     <C56-disconnect >3</C56-disconnect>
   </fc-timers-counters>
   <signalling-protection>
     <confidentiality-protection>true</confidentiality-protection>
     <integrity-protection>false</integrity-protection>
     <mcptt-server-protection>true</mcptt-server-protection>
     <floor-control-protection>false</floor-control-protection>
   </signalling-protection>
 </on-network>

 <off-network>
   <emergency-call>
     <private-cancel-timeout>2</private-cancel-timeout>
     <group-time-limit>3</group-time-limit>
   </emergency-call>
   <private-call>
     <hang-time>3</hang-time>
     <max-duration-with-floor-control>3</max-duration-with-floor-control>
   </private-call>
   <num-levels-priority-hierarchy >3</num-levels-priority-hierarchy>
   <transmit-time>
     <time-limit>3</time-limit>
     <time-warning>3</time-warning>
   </transmit-time>
   <hang-time-warning>3</hang-time-warning> 
   <default-prose-per-packet-priority>
     <mcptt-private-call-signalling >1</mcptt-private-call-signalling>
     <mcptt-private-call-media >2</mcptt-private-call-media>
     <mcptt-emergency-private-call-signalling>3</mcptt-emergency-private-call-signalling>
     <mcptt-emergency-private-call-media >1</mcptt-emergency-private-call-media>
   </default-prose-per-packet-priority>
   <allow-log-metadata>true</allow-log-metadata>
 </off-network>
</service-configuration>
